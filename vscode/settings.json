{
    "settingsSync.ignoredSettings": [
        "window.zoomLevel",
        "workbench.colorTheme"
    ],
    "settingsSync.ignoredExtensions": [
        "atlassian.atlascode"
    ],
    /****** Core Editor ******/
    "telemetry.enableTelemetry": false,
    "window.nativeTabs": true,
    "window.title": "${dirty} ${activeEditorMedium}${separator}${rootName}",
    "editor.fontFamily": "'Fira Code', Menlo, Consolas, Monaco, 'Courier New', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontLigatures": true,
    "editor.fontSize": 16,
    "editor.fontWeight": "500",
    "editor.tabSize": 4,
    "editor.detectIndentation": false,
    "editor.padding.top": 10,
    "editor.padding.bottom": 10,
    "editor.renderWhitespace": "all",
    "editor.insertSpaces": true,
    "editor.rulers": [
        {
            "column": 120,
            "color": "#fa3a3a",
        },
        {
            "column": 100,
            "color": "#ff91006e"
        }
    ],
    "editor.formatOnSave": false,
    "editor.formatOnPaste": true,
    "editor.linkedEditing": true,
    "editor.minimap.side": "right",
    "editor.minimap.enabled": true,
    "editor.minimap.renderCharacters": false,
    "editor.minimap.maxColumn": 120,
    "editor.minimap.showSlider": "always",
    "editor.cursorSmoothCaretAnimation": true,
    "editor.cursorSurroundingLines": 2,
    "editor.multiCursorMergeOverlapping": true,
    "editor.snippetSuggestions": "top",
    "workbench.colorTheme": "Dracula Soft",
    "workbench.sideBar.location": "left",
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.panel.defaultLocation": "left",
    "workbench.settings.editor": "json",
    "workbench.list.multiSelectModifier": "alt",
    "workbench.tree.indent": 12,
    "workbench.tree.renderIndentGuides": "always",
    "explorer.openEditors.visible": 0,
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmDelete": false,
    "breadcrumbs.enabled": true,
    "terminal.external.osxExec": "iTerm.app",
    "terminal.external.windowsExec": "",
    "terminal.integrated.fontFamily": "'Fira Code', Menlo, Consolas, Monaco, 'Courier New', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "terminal.integrated.profiles.osx": {
        "zsh": {
            "path": "zsh",
            "args": [
                "-l"
            ]
        },
    },
    // 'terminal.integrated.shell.osx': "/bin/zsh",
    "terminal.integrated.shell.windows": "C:\\WINDOWS\\System32\\WindowsPowerShell\\v1.0\\powershell.exe",
    "terminal.integrated.minimumContrastRatio": 5,
    "debug.toolBarLocation": "docked",
    "problems.showCurrentInStatus": true,
    "files.hotExit": "off",
    "files.defaultLanguage": "todo",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/.pyc": true,
        "**/pkg.*.js": true,
        "**/pkg.*.js.map": true,
        "**/*.js.html": true,
        "**/coverage/**": true,
        ".nyc_output/": true,
        "**/*.meta": true,
        "**/*.*.meta": true,
        "**/*.unity": true,
        "**/*.unityproj": true,
        "**/*.mat": true,
        "**/*.fbx": true,
        "**/*.FBX": true,
        "**/*.tga": true,
        "**/*.cubemap": true,
        "**/*.prefab": true,
        "**/ProjectSettings": true,
        "**/Temp": true
    },
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/*/**": true
    },
    "files.associations": {
        "*.njk": "njk"
    },
    "search.useIgnoreFiles": true,
    "search.exclude": {
        "**/*.md": true,
        "./TODO": true,
        "**/*.json": true,
        "**/*.map": true,
        "**/*.css": true,
        "**/*.svg": true,
        "**/*.min.js": true,
        "**/*_init.js": true,
        "**/*.html": true,
        "**/*.config": true,
        "**/*.bat": true,
        "**/robots.txt": true,
        "**/Gruntfile.js": true,
        "**/node_modules": true,
        "**/bower_components": true,
        "**/*.code-search": true
    },
    "git.autofetch": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "timeline.excludeSources": [],
    /****** Languages ******/
    "typescript.suggest.completeFunctionCalls": true,
    "javascript.suggest.completeFunctionCalls": true,
    "javascript.format.insertSpaceBeforeAndAfterBinaryOperators": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "html.format.enable": true,
    "[javascript]": {
        "editor.formatOnSave": false,
        "editor.defaultFormatter": "esbenp.prettier-vscode",
    },
    "[html]": {
        "editor.rulers": [
            {
                "column": 120,
                "color": "#fa3a3a",
            },
            {
                "column": 100,
                "color": "#ff91006e"
            }
        ],
        "editor.tabSize": 2
    },
    "html.suggest.html5": true,
    "[scss, css]": {
        "editor.tabSize": 2,
        "editor.formatOnSave": true
    },
    "[json]": {
        "editor.tabSize": 4
    },
    "[markdown]": {
        "editor.wordWrap": "bounded",
        "editor.wordWrapColumn": 100,
        "editor.wrappingStrategy": "simple"
    },
    "emmet.triggerExpansionOnTab": true,
    "emmet.includeLanguages": {
        "njk": "html",
        "hbs": "html"
    },
    /****** Colour Customisations ******/
    "workbench.colorCustomizations": {
        "activityBarBadge.background": "#FF4081",
        "list.activeSelectionForeground": "#FF4081",
        "list.inactiveSelectionForeground": "#FF4081",
        "list.highlightForeground": "#FF4081",
        "scrollbarSlider.activeBackground": "#FF408150",
        "editorSuggestWidget.highlightForeground": "#FF4081",
        "textLink.foreground": "#FF4081",
        "progressBar.background": "#FF4081",
        "pickerGroup.foreground": "#FF4081",
        "tab.activeBorder": "#FF4081",
        "notificationLink.foreground": "#FF4081",
        "editorWidget.resizeBorder": "#FF4081",
        "editorWidget.border": "#FF4081",
        "settings.modifiedItemIndicator": "#FF4081",
        "settings.headerForeground": "#FF4081",
        "panelTitle.activeBorder": "#FF4081",
        "breadcrumb.activeSelectionForeground": "#FF4081",
        "menu.selectionForeground": "#FF4081",
        "menubar.selectionForeground": "#FF4081",
        "editor.findMatchBorder": "#FF4081",
        "selection.background": "#FF408140",
        "debugToolBar.border": "#2f52ca"
    },
    /****** Extensions ******/
    "extensions.ignoreRecommendations": true,
    "togglequotes.chars": [
        "\"",
        "'",
        "`"
    ],
    "todo.indentation": "    ",
    "todohighlight.keywords": [
        {
            "text": "HACK:",
            "color": "red",
            "backgroundColor": "yellow",
            "border": "1px solid red",
            "borderRadius": "2px",
            "isWholeLine": true,
        },
        {
            "text": "TODO:",
            "backgroundColor": "orange"
        },
        {
            "text": "todo:",
            "backgroundColor": "orange"
        },
        {
            "text": "NOTE:",
            "backgroundColor": "blue"
        },
        {
            "text": "FIXME",
            "color": "white",
            "backgroundColor": "#ff69b4",
            "border": "1px solid white",
            "borderRadius": "2px",
            "isWholeLine": true
        }
    ],
    "gitlens.keymap": "chorded",
    "gitlens.advanced.blame.delayAfterEdit": 0,
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false,
        "suppressCommitNotFoundWarning": false,
        "suppressFileNotUnderSourceControlWarning": false,
        "suppressGitVersionWarning": false,
        "suppressLineUncommittedWarning": false,
        "suppressNoRepositoryWarning": false,
        "suppressResultsExplorerNotice": false,
        "suppressShowKeyBindingsNotice": true
    },
    "gitlens.heatmap.toggleMode": "window",
    "gitlens.currentLine.enabled": false,
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.views.lineHistory.enabled": true,
    "materialTheme.accent": "Pink",
}